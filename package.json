{
  "name": "close-similar-tabs",
  "version": "2.3.5",
  "description": "Avoid to have a file opened several times in your tabs. (+setting: by window or everywhere)",
  "main": "main.js",
  "scripts": {
    "dev": "tsx esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && tsx esbuild.config.mjs production",
    "start": "npm i && tsx scripts/start.mjs",
    "acp": "tsx scripts/acp.mts",
    "bacp": "tsx scripts/acp.mts -b",
    "version": "tsx scripts/update-version.mts",
    "release": "tsx scripts/release.mts",
    "test": "tsx scripts/test-plugin.mts"
  },
  "keywords": [],
  "author": "1C0D",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^16.11.6",
    "@types/semver": "^7.5.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "dedent": "^1.5.1",
    "dotenv": "^16.3.1",
    "esbuild": "0.17.3",
    "fs-extra": "^11.2.0",
    "obsidian": "latest",
    "readline": "^1.3.0",
    "semver": "^7.5.4",
    "tslib": "2.4.0",
    "tsx": "^4.7.0",
    "typescript": "4.7.4"
  },
  "dependencies": {"monkey-around": "^2.3.0"}
}